<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product name="MDL IDE" application="org.eclipse.ui.ide.workbench" description="%aboutText">
         <property
               name="cssTheme"
               value="org.eclipse.e4.ui.css.theme.e4_default">
         </property>
         <property
               name="applicationCSSResources"
               value="platform:/plugin/org.eclipse.platform/images/">
         </property>
         <property
             name="aboutText"
             value="%aboutText">
         </property>
         <property
               name="aboutImage"
               value="icons/about_image.png">
         </property>
         <property
               name="appName"
               value="MDL IDE">
         </property>
      </product>
   </extension>
   
</plugin>
