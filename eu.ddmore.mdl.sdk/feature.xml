<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="eu.ddmore.mdl.sdk"
      label="%label"
      version="1.4.0.qualifier"
      provider-name="%provider">

   <description>
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseUrl">
      %license
   </license>

   <url>
      <!-- Externalized url doesn't pass the Feature Editor validation,
      don't commit removal of url="%updateUrl"-->
      <update label="%updateLabel" url="%updateUrl"/>
   </url>

   <includes
         id="eu.ddmore.libpharmml.feature"
         version="0.0.0"/>

   <requires>
      <import plugin="javax.inject" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime" version="3.8.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.swt" version="3.100.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jface" version="3.8.102" match="greaterOrEqual"/>
      <import plugin="org.eclipse.e4.ui.services" version="0.10.3" match="greaterOrEqual"/>
      <import plugin="org.eclipse.e4.ui.workbench" version="0.11.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.e4.core.di" version="1.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.e4.ui.di" version="0.10.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.e4.core.contexts" version="1.2.0" match="greaterOrEqual"/>
      <import plugin="javax.annotation"/>
   </requires>

   <plugin
         id="eu.ddmore.mdleditor.application"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.antlr.runtime"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
