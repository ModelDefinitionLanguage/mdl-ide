# Time to event model
# Interval censoring specified
# PharmML Type1 equation for GROUP_VARIABLES and INDIVIDUAL_VARIABLES
# Hazard function specified in MODEL_PREDICTION 

warfarin_T2E_interval_dat = dataObj{
   DECLARED_VARIABLES{Y}

   DATA_INPUT_VARIABLES{
      ID: {use is id}
      TIME: {use is idv}
      TRT: {use is covariate}
      AMT: {use is ignore}
      RATE: {use is covariate}
      ADDL: {use is addl}
      II: {use is ii}
      WT: {use is covariate}
      DVID: {use is dvid}
      DV: {use is dv, variable=Y}
      MDV: {use is mdv}
      REP: {use is covariate}
      CPX: {use is covariate}
      PCAX: {use is covariate}
      INRX: {use is covariate}
      ICL: {use is covariate}
      IV: {use is covariate}
      ITABS: {use is covariate}
      ITLAG: {use is covariate}
      IF1: {use is covariate}
      IPCA0: {use is covariate}
      IEMAX: {use is covariate}
      IC50: {use is covariate}
      ITEQ: {use is covariate}
      NEVT: {use is covariate}
   }# end DATA_INPUT_VARIABLES

   SOURCE{
      srcFile : {file="warfarin_TTE_exact.csv",
      			inputFormat is nonmemFormat}
   }# end SOURCE
} # end data object

warfarin_T2E_exact_par = parObj{
   STRUCTURAL{
      POP_HBASE : {value=0.1,lo=0}
      POP_BTATRT : {value=0.4, lo=0}
   }# end STRUCTURAL
} # end of parameter object

warfarin_T2E_exact_mdl = mdlObj{
	IDV { TIME }
	
	COVARIATES{
		TRT
	}
	
   VARIABILITY_LEVELS{
	DV : { level=1, type is observation }
   }

   STRUCTURAL_PARAMETERS{
	  POP_HBASE
	  POP_BTATRT
   }# end STRUCTURAL_PARAMETERS

   GROUP_VARIABLES{
	  HAZTRT = POP_HBASE * TRT
   }
   
   INDIVIDUAL_VARIABLES{
	  HBASE = POP_HBASE/365
  }# end INDIVIDUAL_VARIABLES

   MODEL_PREDICTION{
	  HAZ = HBASE * (1+HAZTRT)
   }# end MODEL_PREDICTION

   OBSERVATION{
     Y : {type is tte, hazard = HAZ, event is intervalCensored }
   }# end ESTIMATION

} # end of model object

warfarin_T2E_exact_task = taskObj{
	ESTIMATE{ 
		set algo is saem 
	}
}

warfarin_T2E_exact_mog = mogObj {
	OBJECTS{
		warfarin_T2E_interval_dat : { type is dataObj }
		warfarin_T2E_exact_mdl : { type is mdlObj }
		warfarin_T2E_exact_par : { type is parObj }
		warfarin_T2E_exact_task : { type is taskObj }
	}
}
